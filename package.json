{
  "name": "smartlands-platform",
  "version": "0.1.0",
  "description": "Decentralized smartlands web client.",
  "scripts": {
    "dev": "webpack-dev-server --devtool inline-source-map --inline --content-base public --history-api-fallback --config webpack.config.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --optimize-minimize --config webpack.config.js",
    "server": "node expressProxy",
    "stage": "npm run build && npm run server",
    "lint": "npm run test:lint",
    "start": "npm run dev",
    "clean": "rm -rf ./build",
    "build-electron:linux": "npm run build && cd electron && npm i && npm run build-linux",
    "build-electron:mac": "npm run build && cd electron && npm i && npm run build-mac",
    "build-electron:mac:dmg": "node_modules/.bin/electron-installer-dmg electron/dist/mac/Smartlands-Platform.app SmartlandsPlatform --icon=electron/icons/icon.icns --prune=true --overwrite",
    "build-electron:win:exe": "electron-packager electron/dist/win-unpacked SmartlandsPlatform --overwrite --asar=false --platform=win32 --arch=ia32 --icon=electron/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SmartlandsPlatform\"",
    "build-electron:win": "npm run build && cd electron && npm i && npm run build-win",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint app --fix",
    "test:unit": "mocha --compilers js:babel-core/register **/*.specs.js"
  },
  "repository": "https://github.com/Smartlands-Platform/Wallet.git",
  "keywords": [
    "smartlands",
    "platform",
    "client",
    "webapp",
    "blockchain",
    "decentralized",
    "horizon",
    "lumens"
  ],
  "license": "MIT",
  "devDependencies": {
    "@amcharts/amcharts3-react": "^3.0.0",
    "electron-installer-dmg": "^0.2.1",
    "highcharts": "^6.0.7",
    "react-highcharts": "^16.0.2"
  },
  "dependencies": {
    "@webpack-blocks/babel6": "^0.4.0",
    "@webpack-blocks/css-modules": "^0.4.0",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/extract-text2": "^0.4.0",
    "@webpack-blocks/sass": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "axios": "^0.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bignumber.js": "^6.0.0",
    "chai": "^3.5.0",
    "clipboard": "^1.5.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "decimal.js": "^7.1.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.1",
    "file-saver": "^1.3.8",
    "highcharts": "^6.1.0",
    "highcharts-react": "^0.0.3",
    "highcharts-react-official": "^0.0.1",
    "html-webpack-plugin": "^2.22.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "javascript-time-ago": "^1.0.30",
    "lodash": "^4.17.2",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "react": "^15.6.2",
    "react-addons-create-fragment": "^15.3.2",
    "react-click-outside": "^2.2.0",
    "react-dd-menu": "^2.0.2",
    "react-dom": "^15.3.2",
    "react-highcharts": "^16.0.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-intl": "^2.1.5",
    "react-loading": "^2.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.7",
    "react-slick": "^0.16.0",
    "react-spinners": "^0.3.2",
    "react-tabs": "^2.1.1",
    "react-vis": "^1.9.4",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-form": "^6.5.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "semantic-ui-react": "^0.63.1",
    "stellar-sdk": "^0.8.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "2.2.0"
  }
}
